<!DOCTYPE busconfig PUBLIC "-//freedesktop//DTD D-BUS Bus Configuration 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/busconfig.dtd">
<busconfig>
	<policy user="root">
		<allow own="org.tizen.bp.manager.tizen"/>
	</policy>
	<policy user="app_fw">
		<allow send_destination="org.tizen.bp.manager.tizen" send_interface="org.tizen.bps.bp.manager.interface" send_member="get"/>
	</policy>
	<policy group="app_fw">
		<allow send_destination="org.tizen.bp.manager.tizen" send_interface="org.tizen.bps.bp.manager.interface" send_member="get"/>
	</policy>
</busconfig>